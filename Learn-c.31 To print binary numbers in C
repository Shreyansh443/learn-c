//A program to print binary numbers 
#include <stdio.h>

void printBinary(unsigned int num) {
    for (int i = sizeof(num) * 8 - 1; i >= 0; i--) {
        if (num & (1u << i))
            printf("1");
        else
            printf("0");
    }
}

int main() {
    unsigned int x = 10; // Example
    printf("Binary of %u = ", x);
    printBinary(x);
    return 0;
}