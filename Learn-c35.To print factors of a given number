#include <stdio.h>

void findFactors(int num) {
    printf("Factors of %d are:\n", num);
    for (int i = 1; i <= num; ++i) {
        // Checks if the number is perfectly divisible by i
        if (num % i == 0) {
            printf("%d\n", i);
        }
    }
}

int main() {
    int num;
    printf("Enter a positive integer: ");
    scanf("%d", &num);
    findFactors(num);
    return 0;
}
